{
    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "traceResolution": true,
        "diagnostics": true,
        "listFiles": true,
        "listEmittedFiles": true,
        "pretty": true,
        "locale": "en-us"
    }
}